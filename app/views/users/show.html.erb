<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Contact:</strong>
  <%= @user.contact %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>User Type:</strong>
  <td><% if @user.is_admin %>
      <%= "Admin"%>
      <% elsif @user.is_seeker %>
      <%= "Job Seeker" %>
      <% else %>
      <%= "Business Owner" %>
      <% end %>
  </td>

</p>

<%= link_to 'Edit', profile_edit_path(@user) %> |
<%= link_to 'Back', profile_path %>
