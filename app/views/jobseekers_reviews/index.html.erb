<p id="notice"><%= notice %></p>

<h1>Jobseekers Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Jobseeker</th>
      <th>Bizowner</th>
      <th>Jobseeker review star</th>
      <th>Jobseeker review description</th>
      <th>Job end date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobseekers_reviews.each do |jobseekers_review| %>
      <tr>
        <td><%= jobseekers_review.jobseeker_id %></td>
        <td><%= jobseekers_review.bizowner_id %></td>
        <td><%= jobseekers_review.jobseeker_review_star %></td>
        <td><%= jobseekers_review.jobseeker_review_description %></td>
        <td><%= jobseekers_review.job_end_date %></td>
        <td><%= jobseekers_review.status %></td>
        <td><%= link_to 'Show', jobseekers_review %></td>
        <td><%= link_to 'Edit', edit_jobseekers_review_path(jobseekers_review) %></td>
        <td><%= link_to 'Destroy', jobseekers_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobseekers Review', new_jobseekers_review_path %>
