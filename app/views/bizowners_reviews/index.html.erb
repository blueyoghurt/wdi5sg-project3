<p id="notice"><%= notice %></p>

<h1>Bizowners Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Bizowner Name</th>
      <th>Jobseeker Name</th>
      <th>Bizowner review star</th>
      <th>Business review description</th>
      <th>Job end date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bizowners_reviews.each do |bizowners_review| %>
    <tr>
      <td><%= current_user.name%></td>
      <td><%= bizowners_review.bizowner.user.first_name unless bizowners_review.bizowner.blank? %></td>
      <td><%= bizowners_review.bizowner_review_star %></td>
      <td><%= bizowners_review.business_review_description %></td>
      <td><%= bizowners_review.job_end_date %></td>
      <td><%= bizowners_review.status %></td>
      <td><%= link_to 'Show', bizowners_review %></td>
      <td><%= link_to 'Edit', edit_bizowners_review_path(bizowners_review) %></td>
      <td><%= link_to 'Destroy', bizowners_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if current_user.is_seeker? %>
<%= link_to 'New Bizowners Review', new_bizowners_review_path %>
<% end %>
